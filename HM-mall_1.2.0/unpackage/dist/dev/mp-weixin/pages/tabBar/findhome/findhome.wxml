<view><view class="tabr" style="{{'top:'+(headerTop)+';'}}"><view data-event-opts="{{[['tap',[['switchType',['goods']]]]]}}" class="{{[(typeClass=='goods')?'on':'']}}" bindtap="__e">自习室</view><view data-event-opts="{{[['tap',[['switchType',['shop']]]]]}}" class="{{[(typeClass=='shop')?'on':'']}}" bindtap="__e">办公室</view><view class="{{['border',typeClass]}}"></view></view><view class="place"></view><view class="list"><view class="{{['sub-list goods',subState]}}"><block wx:if="{{srList.length==0}}"><view class="tis">没有数据~</view></block><block wx:for="{{srList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="{{['carrier',typeClass=='goods'?theIndex==index?'open':oldIndex==index?'close':'':'']}}"><view data-event-opts="{{[['tap',[['toStudyroom',['$0'],[[['srList','',index,'srid']]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="info"><view class="title">{{row.srname}}</view><view class="address">{{''+row.sraddress+''}}</view><view class="price-number"><view class="price">{{"￥"+row.price+"/天"}}</view></view></view></view></view></view></block></view><view class="{{['sub-list shop',subState]}}"><block wx:if="{{officeList.length==0}}"><view class="tis">没有数据~</view></block><block wx:for="{{officeList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="{{['carrier',typeClass=='goods'?theIndex==index?'open':oldIndex==index?'close':'':'']}}"><view data-event-opts="{{[['tap',[['toOffice',['$0'],[[['officeList','',index,'officeid']]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="info"><view class="title">{{row.officename}}</view><view class="address">{{''+row.officeaddress+''}}</view><view class="price-number"><view class="price">{{"￥"+row.price+"/月"}}</view></view></view></view></view></view></block></view></view></view>